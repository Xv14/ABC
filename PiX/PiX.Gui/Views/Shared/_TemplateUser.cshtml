<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <meta charset="utf-8">
    <meta name="author" content="pixelhint.com">
    <meta name="description" content="Sublime Stunning free HTML5/CSS3 website template" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/templateUser/")css/reset.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/templateUser/")css/fancybox-thumbs.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/templateUser/")css/fancybox-buttons.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/templateUser/")css/fancybox.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/templateUser/")css/animate.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/templateUser/")css/main.css">

    <script type="text/javascript" src="@Url.Content("~/Content/templateUser/")js/jquery.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/templateUser/")js/fancybox.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/templateUser/")js/fancybox-buttons.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/templateUser/")js/fancybox-media.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/templateUser/")js/fancybox-thumbs.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/templateUser/")js/wow.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/templateUser/")js/main.js"></script>
</head>
<body>
    <section class="billboard dark">
        <header class="wrapper dark">
            <nav>@Html.Partial("_LoginPartial")</nav> 
            <a href="#"><img class="logo" src="@Url.Content("~/Content/templateUser/")img/Xlogo.png" alt="" /></a>
            <nav>
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Profile", "Index", "TODO")</li>
                    <li>@Html.ActionLink("Witnesses", "Index", "TODO")</li>
                    <li>@Html.ActionLink("My Photos", "Index", "Post")</li>
                    <li>@Html.ActionLink("Gallery", "Index", "TODO")</li>
                    <li>@Html.ActionLink("Cards", "Index", "TODO")</li>
                    <li>@Html.ActionLink("About us", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact us", "Contact", "Home")</li>
                </ul>
            </nav>
        </header>

        <div class="caption dark animated wow fadeInDown clearfix">
            <h1>Witness card</h1>
            <p>First Tunisian website for social feedback !</p>
            <hr>
        </div>
        <div class="shadow"></div>
    </section><!--  End billboard  -->

    <div>
        @RenderBody()
    </div>
    
</body>
</html>
